#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/JetDrive/JetDrive/skillbuddy-solana-flutter/publishing/node_modules/.pnpm/@solana-mobile+dapp-store-cli@0.11.0_arweave@1.15.7_bufferutil@4.0.9_fastestsmallesttex_2cf43679a3f417f1339b597c7fecb958/node_modules/@solana-mobile/dapp-store-cli/bin/node_modules:/Volumes/JetDrive/JetDrive/skillbuddy-solana-flutter/publishing/node_modules/.pnpm/@solana-mobile+dapp-store-cli@0.11.0_arweave@1.15.7_bufferutil@4.0.9_fastestsmallesttex_2cf43679a3f417f1339b597c7fecb958/node_modules/@solana-mobile/dapp-store-cli/node_modules:/Volumes/JetDrive/JetDrive/skillbuddy-solana-flutter/publishing/node_modules/.pnpm/@solana-mobile+dapp-store-cli@0.11.0_arweave@1.15.7_bufferutil@4.0.9_fastestsmallesttex_2cf43679a3f417f1339b597c7fecb958/node_modules/@solana-mobile/node_modules:/Volumes/JetDrive/JetDrive/skillbuddy-solana-flutter/publishing/node_modules/.pnpm/@solana-mobile+dapp-store-cli@0.11.0_arweave@1.15.7_bufferutil@4.0.9_fastestsmallesttex_2cf43679a3f417f1339b597c7fecb958/node_modules:/Volumes/JetDrive/JetDrive/skillbuddy-solana-flutter/publishing/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/JetDrive/JetDrive/skillbuddy-solana-flutter/publishing/node_modules/.pnpm/@solana-mobile+dapp-store-cli@0.11.0_arweave@1.15.7_bufferutil@4.0.9_fastestsmallesttex_2cf43679a3f417f1339b597c7fecb958/node_modules/@solana-mobile/dapp-store-cli/bin/node_modules:/Volumes/JetDrive/JetDrive/skillbuddy-solana-flutter/publishing/node_modules/.pnpm/@solana-mobile+dapp-store-cli@0.11.0_arweave@1.15.7_bufferutil@4.0.9_fastestsmallesttex_2cf43679a3f417f1339b597c7fecb958/node_modules/@solana-mobile/dapp-store-cli/node_modules:/Volumes/JetDrive/JetDrive/skillbuddy-solana-flutter/publishing/node_modules/.pnpm/@solana-mobile+dapp-store-cli@0.11.0_arweave@1.15.7_bufferutil@4.0.9_fastestsmallesttex_2cf43679a3f417f1339b597c7fecb958/node_modules/@solana-mobile/node_modules:/Volumes/JetDrive/JetDrive/skillbuddy-solana-flutter/publishing/node_modules/.pnpm/@solana-mobile+dapp-store-cli@0.11.0_arweave@1.15.7_bufferutil@4.0.9_fastestsmallesttex_2cf43679a3f417f1339b597c7fecb958/node_modules:/Volumes/JetDrive/JetDrive/skillbuddy-solana-flutter/publishing/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/dapp-store.js" "$@"
else
  exec node  "$basedir/../../bin/dapp-store.js" "$@"
fi
