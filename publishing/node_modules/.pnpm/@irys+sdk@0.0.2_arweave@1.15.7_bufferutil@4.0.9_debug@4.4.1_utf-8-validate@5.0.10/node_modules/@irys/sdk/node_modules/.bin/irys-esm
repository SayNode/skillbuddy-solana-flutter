#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/JetDrive/JetDrive/skillbuddy-solana-flutter/publishing/node_modules/.pnpm/@irys+sdk@0.0.2_arweave@1.15.7_bufferutil@4.0.9_debug@4.4.1_utf-8-validate@5.0.10/node_modules/@irys/sdk/build/esm/node/node_modules:/Volumes/JetDrive/JetDrive/skillbuddy-solana-flutter/publishing/node_modules/.pnpm/@irys+sdk@0.0.2_arweave@1.15.7_bufferutil@4.0.9_debug@4.4.1_utf-8-validate@5.0.10/node_modules/@irys/sdk/build/esm/node_modules:/Volumes/JetDrive/JetDrive/skillbuddy-solana-flutter/publishing/node_modules/.pnpm/@irys+sdk@0.0.2_arweave@1.15.7_bufferutil@4.0.9_debug@4.4.1_utf-8-validate@5.0.10/node_modules/@irys/sdk/build/node_modules:/Volumes/JetDrive/JetDrive/skillbuddy-solana-flutter/publishing/node_modules/.pnpm/@irys+sdk@0.0.2_arweave@1.15.7_bufferutil@4.0.9_debug@4.4.1_utf-8-validate@5.0.10/node_modules/@irys/sdk/node_modules:/Volumes/JetDrive/JetDrive/skillbuddy-solana-flutter/publishing/node_modules/.pnpm/@irys+sdk@0.0.2_arweave@1.15.7_bufferutil@4.0.9_debug@4.4.1_utf-8-validate@5.0.10/node_modules/@irys/node_modules:/Volumes/JetDrive/JetDrive/skillbuddy-solana-flutter/publishing/node_modules/.pnpm/@irys+sdk@0.0.2_arweave@1.15.7_bufferutil@4.0.9_debug@4.4.1_utf-8-validate@5.0.10/node_modules:/Volumes/JetDrive/JetDrive/skillbuddy-solana-flutter/publishing/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/JetDrive/JetDrive/skillbuddy-solana-flutter/publishing/node_modules/.pnpm/@irys+sdk@0.0.2_arweave@1.15.7_bufferutil@4.0.9_debug@4.4.1_utf-8-validate@5.0.10/node_modules/@irys/sdk/build/esm/node/node_modules:/Volumes/JetDrive/JetDrive/skillbuddy-solana-flutter/publishing/node_modules/.pnpm/@irys+sdk@0.0.2_arweave@1.15.7_bufferutil@4.0.9_debug@4.4.1_utf-8-validate@5.0.10/node_modules/@irys/sdk/build/esm/node_modules:/Volumes/JetDrive/JetDrive/skillbuddy-solana-flutter/publishing/node_modules/.pnpm/@irys+sdk@0.0.2_arweave@1.15.7_bufferutil@4.0.9_debug@4.4.1_utf-8-validate@5.0.10/node_modules/@irys/sdk/build/node_modules:/Volumes/JetDrive/JetDrive/skillbuddy-solana-flutter/publishing/node_modules/.pnpm/@irys+sdk@0.0.2_arweave@1.15.7_bufferutil@4.0.9_debug@4.4.1_utf-8-validate@5.0.10/node_modules/@irys/sdk/node_modules:/Volumes/JetDrive/JetDrive/skillbuddy-solana-flutter/publishing/node_modules/.pnpm/@irys+sdk@0.0.2_arweave@1.15.7_bufferutil@4.0.9_debug@4.4.1_utf-8-validate@5.0.10/node_modules/@irys/node_modules:/Volumes/JetDrive/JetDrive/skillbuddy-solana-flutter/publishing/node_modules/.pnpm/@irys+sdk@0.0.2_arweave@1.15.7_bufferutil@4.0.9_debug@4.4.1_utf-8-validate@5.0.10/node_modules:/Volumes/JetDrive/JetDrive/skillbuddy-solana-flutter/publishing/node_modules/.pnpm/node_modules:$NODE_PATH"
